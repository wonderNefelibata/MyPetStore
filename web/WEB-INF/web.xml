<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>MainFormServlet</servlet-name>
        <servlet-class>csu.web.mypetstore.web.servlet.MainFormServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>CategoryFromServlet</servlet-name>
        <servlet-class>csu.web.mypetstore.web.servlet.CategoryFromServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ProductFromServlet</servlet-name>
        <servlet-class>csu.web.mypetstore.web.servlet.ProductFormServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ItemFromServlet</servlet-name>
        <servlet-class>csu.web.mypetstore.web.servlet.ItemFormServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>AddItemToCartServlet</servlet-name>
        <servlet-class>csu.web.mypetstore.web.servlet.AddItemToCartServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>CartFormServlet</servlet-name>
        <servlet-class>csu.web.mypetstore.web.servlet.CartFormServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>UpdateCartServlet</servlet-name>
        <servlet-class>csu.web.mypetstore.web.servlet.UpdateCartServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>RemoveCartItemServlet</servlet-name>
        <servlet-class>csu.web.mypetstore.web.servlet.RemoveCartItemServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>SignOnFormServlet</servlet-name>
        <servlet-class>csu.web.mypetstore.web.servlet.SignOnFormServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>NewOrderFormServlet</servlet-name>
        <servlet-class>csu.web.mypetstore.web.servlet.NewOrderFormServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>SignOnServlet</servlet-name>
        <servlet-class>csu.web.mypetstore.web.servlet.SignOnServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>SearchProductServlet</servlet-name>
        <servlet-class>csu.web.mypetstore.web.servlet.SearchProductServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ViewProductServlet</servlet-name>
        <servlet-class>csu.web.mypetstore.web.servlet.ViewProductServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>csu.web.mypetstore.web.servlet.RegisterServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>RegisterFormServlet</servlet-name>
        <servlet-class>csu.web.mypetstore.web.servlet.RegisterFormServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>SignOutFormServlet</servlet-name>
        <servlet-class>csu.web.mypetstore.web.servlet.SignOutFormServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>NewOrderServlet</servlet-name>
        <servlet-class>csu.web.mypetstore.web.servlet.NewOrderServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ConfirmOrderFormServlet</servlet-name>
        <servlet-class>csu.web.mypetstore.web.servlet.ConfirmOrderFormServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ListOrdersServlet</servlet-name>
        <servlet-class>csu.web.mypetstore.web.servlet.ListOrdersServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ViewOrderServlet</servlet-name>
        <servlet-class>csu.web.mypetstore.web.servlet.ViewOrderServlet</servlet-class>
    </servlet>



    <servlet-mapping>
        <servlet-name>MainFormServlet</servlet-name>
        <url-pattern>/mainForm</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>CategoryFromServlet</servlet-name>
        <url-pattern>/categoryForm</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>ProductFromServlet</servlet-name>
        <url-pattern>/productForm</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>ItemFromServlet</servlet-name>
        <url-pattern>/itemForm</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>AddItemToCartServlet</servlet-name>
        <url-pattern>/addItemToCart</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>CartFormServlet</servlet-name>
        <url-pattern>/cartForm</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>UpdateCartServlet</servlet-name>
        <url-pattern>/updateCart</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>RemoveCartItemServlet</servlet-name>
        <url-pattern>/removeCartItem</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>SignOnFormServlet</servlet-name>
        <url-pattern>/signonForm</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>NewOrderFormServlet</servlet-name>
        <url-pattern>/newOrderForm</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>SignOnServlet</servlet-name>
        <url-pattern>/signOn</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>SearchProductServlet</servlet-name>
        <url-pattern>/searchProducts</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>ViewProductServlet</servlet-name>
        <url-pattern>/viewProduct</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>RegisterFormServlet</servlet-name>
        <url-pattern>/registerForm</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>SignOutFormServlet</servlet-name>
        <url-pattern>/signOutForm</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>NewOrderServlet</servlet-name>
        <url-pattern>/newOrder</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>ConfirmOrderFormServlet</servlet-name>
        <url-pattern>/confirmOrderForm</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>ListOrdersServlet</servlet-name>
        <url-pattern>/listOrders</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>ViewOrderServlet</servlet-name>
        <url-pattern>/viewOrder</url-pattern>
    </servlet-mapping>
</web-app>